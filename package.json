{
  "name": "slack-notifier",
  "version": "1.0.0",
  "description": "",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "start": "webpack -w",
    "test": "jest",
    "build": "NODE_ENV=${NODE_ENV:-production} webpack",
    "package": "sam package --template-file template.yaml --s3-bucket lambda-function-deploy-packages --output-template-file template.packaged.yaml",
    "deploy": "sam deploy --template-file template.packaged.yaml --stack-name ${npm_package_name} --capabilities CAPABILITY_IAM",
    "clean": "rm -rf -v dist/* && rm -f template.packaged.yaml",
    "sam-deploy": "yarn clean && yarn build && yarn package && yarn deploy",
    "local-invoke": "sam local invoke --event ./events/sns.json --env-vars env/env.json"
  },
  "repository": {},
  "author": "o-hayato <preasper0+github@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "typed-slack": "^0.1.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.3",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/chai": "^4.1.3",
    "@types/jest": "^23.1.4",
    "@types/node": "^12.12.5",
    "aws-sdk": "^2.814.0",
    "axios-mock-adapter": "^1.17.0",
    "jest": "^23.3.0",
    "prettier": "^1.18.2",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.1.1",
    "yaml-cfn": "^0.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "main": "index.js"
}
